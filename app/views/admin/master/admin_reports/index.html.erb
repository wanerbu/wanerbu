    <div class="filter">
      <%= f.datagrid_label filter %>
      <%= f.datagrid_filter filter %>
    </div>
    <div class="clear"></div>
  <% end -%>

  <div class="filter">
    <%= f.submit 'select' %>
  </div>
  <div class="clear"></div>
<% end -%>

<h3>Admins Report</h3>

