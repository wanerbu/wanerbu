  <%= semantic_form_for([:user, :master, @court], :html => { :class => 'form-horizontal' }) do |f| %>
      <%= f.input :min_unit %>
    <%= f.actions do %>
      <%= f.action :submit, :button_html => { :class => "btn btn-primary" }, :as => :button %>
      <%= f.action :cancel, :as => :link %>
    <% end -%>
  <% end %>
