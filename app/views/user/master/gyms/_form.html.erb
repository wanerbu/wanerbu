  <%= semantic_form_for([:user, :master, @gym], :html => { :class => 'form-horizontal' }) do |f| %>
    <%= f.inputs  do %>
      <%= f.input :name, :required => true, :input_html => {:class=> ""}%>
      <%= f.input :intro, :required => true, :input_html => { :class => "" } %>
      <%= f.input :address, :required => true, :input_html => { :class => "" } %>
      <%= f.input :telephone, :required => true,:input_html => { :class => "" } %>
      <%= f.input :open_time, :as => :time_select,:required => true,:input_html => { :class => "" } %>
      <%= f.input :close_time, :as => :time_select,:required => true,:input_html => { :class => "" } %>
    <% end -%>
    <%= f.actions do %>
      <%= f.action :submit, :button_html => { :class => "btn btn-primary" }, :as => :button %>
      <%= f.action :cancel, :as => :link %>
    <% end -%>
  <% end %>
